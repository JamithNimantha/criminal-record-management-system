<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="706.0" prefWidth="976.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.gdse.client.controller.user.OfficerController">
   <children>
      <ImageView fx:id="imgPhoto" fitHeight="264.0" fitWidth="234.0" layoutX="19.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../assests/icons8-administrator-male-100.png" />
         </image>
      </ImageView>
      <JFXTextField fx:id="txtID" labelFloat="true" layoutX="266.0" layoutY="33.0" onAction="#txtIDOnAction" prefHeight="26.0" prefWidth="200.0" promptText="ID">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="btnSearch" layoutX="496.0" layoutY="31.0" onAction="#btnSearchOnAction" prefHeight="26.0" prefWidth="78.0" styleClass="button-search" stylesheets="@../../css/style.css" text="Search" />
      <JFXTextField fx:id="txtName" editable="false" labelFloat="true" layoutX="265.0" layoutY="78.0" prefHeight="26.0" prefWidth="680.0" promptText="Full Name">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtPosition" editable="false" labelFloat="true" layoutX="265.0" layoutY="122.0" prefHeight="26.0" prefWidth="399.0" promptText="Position">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtDepartment" editable="false" labelFloat="true" layoutX="264.0" layoutY="164.0" prefHeight="26.0" prefWidth="680.0" promptText="Department">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtAddress" editable="false" labelFloat="true" layoutX="264.0" layoutY="205.0" prefHeight="26.0" prefWidth="680.0" promptText="Official Address">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtEmail" editable="false" labelFloat="true" layoutX="265.0" layoutY="250.0" prefHeight="26.0" prefWidth="365.0" promptText="E-Mail">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtTel" editable="false" labelFloat="true" layoutX="663.0" layoutY="250.0" prefHeight="26.0" prefWidth="286.0" promptText="Telphone Number">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXTextField>
      <TableView fx:id="tblUser" layoutX="16.0" layoutY="295.0" onMouseClicked="#tblUserOnMouseClicked" prefHeight="402.0" prefWidth="948.0">
         <columns>
            <TableColumn prefWidth="104.0" text="ID" />
            <TableColumn prefWidth="144.0" text="Name" />
            <TableColumn prefWidth="131.0" text="Position" />
            <TableColumn prefWidth="165.0" text="Department" />
            <TableColumn prefWidth="135.0" text="Address" />
            <TableColumn prefWidth="102.0" text="E-Mail" />
            <TableColumn prefWidth="166.0" text="Telphone" />
         </columns>
      </TableView>
      <JFXTextField fx:id="txtPermissionLevel" editable="false" labelFloat="true" layoutX="695.0" layoutY="123.0" prefHeight="26.0" prefWidth="246.0" promptText="Permission Level">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXTextField>
   </children>
</AnchorPane>
